{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyPowerUserOutsideUS",
      "Effect": "Allow",
      "NotAction": [
        "iam:*",
        "organizations:*"
      ],
      "Resource": "*",
        "Condition": {"StringEquals": {"aws:RequestedRegion": [
            "us-east-1"
        ]}}
    },
    {
      "Effect": "Allow",
      "Action": [
        "iam:CreateServiceLinkedRole",
        "iam:DeleteServiceLinkedRole",
        "iam:ListRoles",
        "organizations:DescribeOrganization"
      ],
      "Resource": "*",
        "Condition": {"StringEquals": {"aws:RequestedRegion": [
            "us-east-1"
        ]}}
    }
  ]
}